{
  "description": "Roblox Player via Sober Flatpak inside a Kasm container.",
  "docker_registry": "https://index.docker.io/v1/",
  "image_src": "sober-icon.svg",
  "name": "yourdockerhubusername/sober-kasm:latest",
  "run_config": {
    "cap_add": ["SYS_ADMIN", "NET_ADMIN"],
    "hostname": "kasm",
    "privileged": true
  },
  "exec_config": {
    "assign": {
      "cmd": "bash -c 'flatpak install -y flathub org.vinegarhq.Sober'"
    },
    "go": {
      "cmd": "bash -c 'flatpak run org.vinegarhq.Sober'"
    }
  },
  "categories": ["Gaming", "Roblox"],
  "friendly_name": "Roblox (Sober)",
  "architecture": ["amd64"],
  "compatibility": ["1.13.x", "1.14.x", "1.15.x"],
  "uncompressed_size_mb": 1500,
  "notes": "Runs Roblox using the Sober Flatpak. Performance may vary depending on GPU passthrough and container permissions."
}